create table Player(
ID INTEGER PRIMARY KEY AUTOINCREMENT,
login varchar(30),
password TEXT,
email TEXT,
signUpTime varchar(24),
active varchar(1)
);

create table Game(
ID INTEGER PRIMARY KEY AUTOINCREMENT,
player1ID INTEGER,
player2ID INTEGER,
player3ID INTEGER,
player4ID INTEGER,
matchDate varchar(24),
winnerID INTEGER
);  